<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Background Colors -->
    <Color x:Key="primaryBackColor1">#0D0D0D</Color>
    <Color x:Key="primaryBackColor2">#1A1A1A</Color>
    <Color x:Key="secondaryBackColor1">#111111</Color>
    <Color x:Key="secondaryBackColor2">#141414</Color>

    <!-- Window Border Colors -->
    <Color x:Key="winBorderColor1">#00897b</Color>
    <Color x:Key="winBorderColor2">#00564d</Color>
    <Color x:Key="winBorderColor3">#282828</Color>

    <!-- Color Palette -->
    <SolidColorBrush x:Key="color1" Color="#9C27B0" />
    <SolidColorBrush x:Key="color2" Color="#7B1FA2" />
    <SolidColorBrush x:Key="color3" Color="#6A1B9A" />
    <SolidColorBrush x:Key="color4" Color="#4CAF50" />
    <SolidColorBrush x:Key="color5" Color="#388E3C" />
    <SolidColorBrush x:Key="color6" Color="#2E7D32" />
    <SolidColorBrush x:Key="color7" Color="#512DA8" />
    <SolidColorBrush x:Key="color8" Color="#303F9F" />
    <SolidColorBrush x:Key="color9" Color="#689F38" />
    <SolidColorBrush x:Key="color10" Color="#303030" />
    
    <!-- Font Colors -->
    <SolidColorBrush x:Key="titleColor1" Color="#FFFFFF" />
    <SolidColorBrush x:Key="titleColor2" Color="#E0E0E0" />
    <SolidColorBrush x:Key="titleColor3" Color="#BDBDBD" />
    <SolidColorBrush x:Key="plainTextColor1" Color="#FFFFFF" />
    <SolidColorBrush x:Key="plainTextColor2" Color="#E0E0E0" />
    <SolidColorBrush x:Key="plainTextColor3" Color="#BDBDBD" />
    
    <!-- Panel Colors -->
    <SolidColorBrush x:Key="panelColor" Color="#0A0A0A" />
    <SolidColorBrush x:Key="panelOverColor" Color="#050505" />
    <SolidColorBrush x:Key="panelActiveColor" Color="#1A1A1A" />

    <!-- Button Colors -->

    <!-- TextBox  -->

    <!-- Etc -->

</ResourceDictionary>